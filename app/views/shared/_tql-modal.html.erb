<div id="tql-modal">
  <div id="tql-modal-content">
    <%= simple_form_for @medium, :url => media_path, :method => :post, remote: true do |f| %>
      <%= f.hidden_field :quest, value: @quest.id %>
      <%= f.hidden_field :team_quest_link, value: @team_quest_link_id %>
      <%= f.input :attached_file, as: :file, label: false, input_html: {class: "main-button"}%>
      <%= button_tag "CONFIRM", type: 'submit', id: "btn-send-media", class: 'main-button' %>
    <% end %>
  </div>
  <div id="tql-modal-close">CANCEL</div>
</div>
